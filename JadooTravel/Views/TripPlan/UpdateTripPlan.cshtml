@model JadooTravel.Dtos.TripPlanDtos.GetTripPlanByIdDto
@{
    ViewData["Title"] = "UpdateTripPlan";
    Layout = "~/Views/Admin/Index.cshtml";
}

<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />

<div class="container">
    <br />
    <br />
    <form method="post">
        <input type="hidden" asp-for="TripPlanId"/>
        <input type="text" asp-for="Title" placeholder="Başlık Giriniz" class="form-control" />
        <br />
        <input type="text" asp-for="Description" placeholder="Açıklama Giriniz" class="form-control" />
        <br />
        <input type="text" asp-for="IconUrl" placeholder="Ikon Url Giriniz" class="form-control" />
        <br />

        <button class="btn btn-primary">Servisi Ekle</button>
    </form>
</div>


